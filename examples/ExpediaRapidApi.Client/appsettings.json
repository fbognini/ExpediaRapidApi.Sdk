{
  "AllowedHosts": "*",
  "ExpediaRapidApiSettings": {
    "EnvironmentName": "STG",
    "ApiKey": {
      "ApiKey": "",
      "ApiSecret": ""
    },
    "OAuth": {
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "Serilog": {
    "Using": [],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "ExpediaRapidApi.Client"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire": "Warning",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.log",
          "outputTemplate": "{Timestamp} [{Level:u3}] [{RequestId}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "retainedFileCountLimit": 5
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": ""
        }
      }
    ]
  }
}
